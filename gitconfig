	email = will@cozy.co
[push]
	default = current
[pull]
	rebase = true
[core]
	excludesfile = /Users/will/.gitignore_global
[alias]
	br = branch
	cm = commit -v
	cma = commit -va
	cam = commit -vam
	co = checkout
	cob = checkout -b
        com = co master
        cod = co develop
	lg = log --stat --max-count=5
	lgp = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	lgg = log --graph --max-count=5
	lgo = log --stat --oneline
	pr = pull-request
